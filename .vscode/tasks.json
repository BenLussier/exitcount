{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "Flutter-Launch-All",
      "dependsOn": [
        "Flutter-Launch-iPhone",
        "Flutter-Launch-iPad",
        "Flutter-Launch-Pixel2XL",
        // "Flutter-Launch-Web"
      ]
    },
    {
      "label": "Flutter-Launch-iOS",
      "dependsOn": [
        "Flutter-Launch-iPhone",
        "Flutter-Launch-iPad",
      ]
    },
    {
      "label": "Flutter-Launch-iPhone",
      "type": "shell",
      "command": "flutter run -d 'iPhone' "
    },
    {
      "label": "Flutter-Launch-iPad",
      "type": "shell",
      "command": "flutter run -d 'iPad' "
    },
    {
      "label": "Flutter-Launch-Android",
      "type": "shell",
      "command": "flutter run -d 'emulator-name' "
    },
    {
      "label": "Flutter-Launch-Pixel2XL",
      "type": "shell",
      "command": "flutter run -d 'Pixel 2 XL' "
    },
    // {
    //   "label": "Flutter-Launch-Web",
    //   "type": "shell",
    //   "command": "flutter run -d 'Chrome' "
    // }
  ]
}